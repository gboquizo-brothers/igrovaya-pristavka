{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.3",
        "ext-json": "*",
        "appstract/laravel-blade-directives": "^1.7",
        "astrotomic/laravel-translatable": "^11.6",
        "bensampo/laravel-enum": "^1.26",
        "consoletvs/charts": "^6.5",
        "davejamesmiller/laravel-breadcrumbs": "^5.3",
        "fideloper/proxy": "^4.0",
        "jenssegers/agent": "^2.6",
        "laravel/framework": "^6.0",
        "laravel/tinker": "^1.0",
        "mcamara/laravel-localization": "^1.4",
        "spatie/laravel-medialibrary": "^7.14",
        "thepinecode/i18n": "^0.6.7",
        "tightenco/ziggy": "^0.8.1",
        "yajra/laravel-datatables": "^1.5"
    },
    "require-dev": {
        "atehnix/laravel-stubs": "^6.4",
        "barryvdh/laravel-ide-helper": "^2.6",
        "facade/ignition": "^1.4",
        "friendsofphp/php-cs-fixer": "^2.16",
        "fruitcake/laravel-telescope-toolbar": "^1.1",
        "fzaninotto/faker": "^1.4",
        "gboquizo/sweet-routes": "^2.0",
        "gboquizo/morpheable": "@dev",
        "laravel/homestead": "^9.4",
        "laravel/ui": "^1.1",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^3.0",
        "phpmd/phpmd": "^2.7",
        "phpunit/phpunit": "^8.0",
        "squizlabs/php_codesniffer": "^3.5"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "repositories": [
        {
            "type": "path",
            "url": "packages/morpheable"
        }
    ],
    "autoload": {
        "psr-4": {
            "App\\": ["core/", "app/"]
        },
        "classmap": [
            "database/seeds",
            "database/factories"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
